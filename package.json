    "dotenv": "^16.0.3",
    "google-libphonenumber": "^3.2.32",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "lodash": "^4.17.21",
    "messagebird": "^4.0.1",
    "nanoid": "3",
    "nestjs-minio-client": "^2.0.0",
    "nestjs-stripe": "^1.0.0",
    "nodemailer": "^6.9.1",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
